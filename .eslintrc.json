{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"camelcase": "off",
		"max-len": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-global-selector": "off",
		"no-prototype-builtins": "off",
		"no-underscore-dangle": "off",
		"no-unused-vars": "off",
		"no-jquery/no-done-fail": "warn"
	},
	"globals": {
		"ext": "readonly",
		"jQuery": "readonly",
		"mediaWiki": "readonly",
		"OOJSPlus": "readonly"
	}
}
